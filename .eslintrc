{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:security/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "security"  
  ],
  "rules": {
    "no-unused-vars": "warn",
    "eqeqeq": "warn",
    "no-dupe-args": "warn",
    "no-dupe-else-if": "warn",
    "no-unreachable": "warn",
    "no-empty-function": "warn",
    "no-useless-return": "warn",
    "react/prop-types": "off",
    "no-irregular-whitespace": "warn"
  }
}
